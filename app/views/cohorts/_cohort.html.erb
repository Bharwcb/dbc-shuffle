<% if @groupings.present? %>
 <% @groupings.each do |group| %>
   <div class="panel panel-block">
    <% group.each do |student|%>
        <div class="panel-title"><%= student.try(:name) %></div>
    <% end %>
  </div>
 <% end %>
<% else %>
  <h2><%=@cohort.name %><h2>
  <h4><%= @cohort.location.name %></h4>
  <ul>
      <% @cohort.students.each do |student| %>
            <li>
                <%= student.name %>
                <%= student.group_id %>
            </li>
      <% end  %>
  </ul>
<% end %>